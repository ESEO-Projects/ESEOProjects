      <div class="content">
        <div class="container-fluid">
          <br>
          {% block body %}{% endblock %}
        </div><!-- /.container-fluid -->
      </div>
      <!-- /.content -->
    </div>
    <!-- /.content-wrapper -->
    <footer class="main-footer">
      <div class="float-right d-none d-sm-inline">
